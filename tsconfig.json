{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "lib": ["ESNext", "dom"],
    "moduleResolution": "node",
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ]
}